{
    "java.project.referencedLibraries": [
        "Dependencies/**/*.jar",
        "Dependencies/libraries/**/*.jar"
    ],
    "java.project.sourcePaths": [
        "Nerd/src",
        "NerdDemos/src",
        "NerdExtraModules/ScenesAndLayers",
        "NerdExtraModules/ECS"
    ],
    "java.cleanup.actionsOnSave": [
        "addOverride",
        "invertEquals",
        "addDeprecated",
        "qualifyMembers",
        "tryWithResource",
        "addFinalModifier",
        "lambdaExpression",
        "qualifyStaticMembers",
        "instanceofPatternMatch",
        "stringConcatToTextBlock",
    ],
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xms128M -Xmx1024M",
