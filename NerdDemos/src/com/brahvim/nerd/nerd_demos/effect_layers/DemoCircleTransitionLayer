package com.brahvim.nerd.nerd_demos.effect_layers;

import com.brahvim.nerd.framework.scene_layer_api.NerdScene;
import com.brahvim.nerd.framework.scene_layer_api.renderer_specific_impls.layers.NerdP3dLayer;

import processing.opengl.PGraphics3D;

public class DemoCircleTransitionLayer extends NerdP3dLayer {

	protected DemoCircleTransitionLayer(NerdScene<PGraphics3D> p_scene) {
		super(p_scene);
	}

	@Override
	protected void draw() {
		super.GRAPHICS.begin2d();
		super.GRAPHICS.fill(0);

		super.GRAPHICS.end2d();
	}

}
